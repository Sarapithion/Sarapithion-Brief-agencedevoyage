{% extends 'base.html.twig' %}

{% block title %}AdvReservation index{% endblock %}

{% block body %}
    <h1>AdvReservation index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Message</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for adv_reservation in adv_reservations %}
            <tr>
                <td>{{ adv_reservation.id }}</td>
                <td>{{ adv_reservation.Message }}</td>
                <td>
                    <a href="{{ path('app_adv_reservation_show', {'id': adv_reservation.id}) }}">show</a>
                    <a href="{{ path('app_adv_reservation_edit', {'id': adv_reservation.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_adv_reservation_new') }}">Create new</a>
{% endblock %}
